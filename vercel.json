{
    "version": 2,
    "buildCommand": "cd frontend && npm run build",
    "outputDirectory": "frontend/build",
    "installCommand": "pip3 install --upgrade pip && pip3 install psycopg2-binary==2.9.7 --force-reinstall --no-deps && pip3 install -r requirements.txt",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/index.html"
        }
    ]
}