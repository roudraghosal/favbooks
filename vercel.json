{
    "version": 2,
    "buildCommand": "cd frontend && npm run build",
    "outputDirectory": "frontend/build",
    "installCommand": "python3 -m pip install --upgrade pip --only-binary=all && python3 -m pip install --only-binary=all -r requirements.txt",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/index.html"
        }
    ]
}